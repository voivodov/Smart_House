@model IEnumerable<SmartHouse.Web.Models.ArduinoDataModelPost>

@{
    ViewBag.Title = "View";
}

<h2>Arduino Data Mapping</h2>

<table class="table table-hover table-bordered">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.time)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.@in)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.@out)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.hum)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.room1)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.time)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.@in)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.@out)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.hum)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.room1)
        </td>
    </tr>
}

</table>
